{
    "name": "netgen/siteaccess-routes-bundle",
    "description": "Netgen Siteaccess Routes Bundle is an eZ Platform bundle which allows you to specify in which siteaccesses or siteaccess groups can a route be used",
    "type": "ezplatform-bundle",
    "keywords": [
        "ezpublish",
        "ezplatform",
        "netgen",
        "siteaccess-routes-bundle",
        "siteaccess"
    ],
    "homepage": "https://github.com/netgen/NetgenSiteAccessRoutesBundle",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://www.netgenlabs.com"
        }
    ],
    "require": {
        "ezsystems/ezpublish-kernel": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Netgen\\Bundle\\SiteAccessRoutesBundle\\": "bundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\Bundle\\SiteAccessRoutesBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
